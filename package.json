{
  "name": "imdb-util-js",
  "version": "1.0.0",
  "description": "A tool for pulling JSON data from the IMDb datasets.",
  "main": "search.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "tconst": "node ./src/search/tconst.js",
    "mini": "node ./src/search/mini_title.js",
    "title": "node ./src/search/title.js",
    "schema": "mysql -u root < ./src/setup/schema.sql",
    "import": "node ./src/setup/import.js",
    "teardown": "node ./src/teardown/cleardb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harmelodic/imdb-util.git"
  },
  "keywords": [
    "imdb"
  ],
  "author": "Harmelodic",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Harmelodic/imdb-util/issues"
  },
  "homepage": "https://github.com/Harmelodic/imdb-util#README",
  "dependencies": {
    "mysql": "2.18.1",
    "chromedriver": "131.0.5",
    "selenium-webdriver": "4.27.0"
  }
}
